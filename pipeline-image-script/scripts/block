#!/bin/bash
#Author: Mark Cu

block=$1

if [[ "$block" == "" ]]
then
cat << "EOF"
___________________________________________________________________________________________
EOF
fi

if [[ "$block" == "1" ]]
then
cat << "EOF"
███████████████████████████████████████████████████████████████████████████████████████████
EOF
fi

if [[ "$block" == "2" ]]
then
cat << "EOF"
░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░░
EOF
fi

if [[ "$block" == "3" ]]
then
cat << "EOF"
═══════════════════════════════════════════════════════════════════════════════════════════
EOF
fi

